<% provide :title, t(".booking_title") %>
<h1><%= t(".booking_title") %></h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t(".fullname") %></th>
      <th><%= t(".phone_number") %></th>
      <th><%= t(".address") %></th>
      <th><%= t(".note") %></th>
      <th><%= t(".name_tour") %></th>
      <th><%= t(".start_day") %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @bookings %>
  </tbody>
</table>
<div>
  <%= will_paginate %>
</div>
